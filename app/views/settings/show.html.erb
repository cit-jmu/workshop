<div class="page-header">
  <h1>
    Settings
    <small>View an application setting</small>
  </h1>
</div>

<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @setting.name %>
</p>

<p>
  <strong>Value:</strong>
  <%= @setting.value %>
</p>

<p>
  <strong>Last changed by:</strong>
  <%= @setting.last_changed_by %>
</p>

<%= link_to 'Edit', edit_setting_path(@setting), class: 'btn btn-primary' %>
<%= link_to 'Back', settings_path, class: 'btn btn-default' %>
